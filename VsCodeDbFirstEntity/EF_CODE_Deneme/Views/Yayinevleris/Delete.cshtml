@model EF_CODE_Deneme.Models.Yayinevleri
@{
    ViewData["Title"] = "Delete";
}
<div class="row">
    <div class="col-sm-4">
        Uye Id:
    </div>
    <div class="col-sm-4">
        @Model.Id
    </div>
</div>
<div class="row">
    <div class="col-sm-4">
        Ad Soyad:
    </div>
    <div class="col-sm-4">
        @Model.Ad
    </div>
</div>
<div class="row">
    <div class="col-sm-4">
        Adres:
    </div>
    <div class="col-sm-4">
        @Model.Adres
    </div>
</div>
<div class="row">
    <div class="col-sm-4">
        Telefon:
    </div>
    <div class="col-sm-4">
        @Model.Tel
    </div>
</div>
<form asp-action="DeleteYayinevi">
    <input type="hidden" asp-for="Id">
    <input type="submit" value="Sil" class="btn btn-danger">
</form>