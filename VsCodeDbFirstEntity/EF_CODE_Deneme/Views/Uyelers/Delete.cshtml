@model EF_CODE_Deneme.Models.Uyeler
@{
    ViewData["Title"] = "Delete";
}

<div class="row">
    <div class="col-sm-4">
        Uye Id:
    </div>
    <div class="col-sm-4">
        @Model.Id
    </div>
</div>
<div class="row">
    <div class="col-sm-4">
        Ad Soyad:
    </div>
    <div class="col-sm-4">
        @Model.AdSoyad
    </div>
</div>
<div class="row">
    <div class="col-sm-4">
        Cinsiyet:
    </div>
    <div class="col-sm-4">
        @Model.Cinsiyet
    </div>
</div>
<form asp-action="DeleteUye">
    <input type="hidden" asp-for="Id">
    <input type="submit" value="Sil" class="btn btn-danger">
</form>